{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "func-names": [0],
    "prefer-arrow-callback": [0],
    "arrow-body-style": [0],
    "max-len": [0],
    "react/prefer-stateless-function": [0]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
